    "@unocss/eslint-plugin": "^65.4.2",
    "astro-eslint-parser": "^1.1.0",
    "bumpp": "^9.10.1",
    "eslint": "^9.18.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-typegen": "^1.0.0",
    "execa": "^9.5.2",
