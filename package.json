{
  "name": "@xwbx/eslint-config",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@9.7.0",
  "description": "xwbx's ESLint config",
  "author": "xwbx <1677759063@qq.com> (https://github.com/daflyinbed/)",
  "license": "MIT",
  "homepage": "https://github.com/daflyinbed/eslint-config",
  "keywords": ["eslint-config"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm run typegen && tsup --format esm,cjs --clean --dts",
    "stub": "tsup --format esm",
    "dev": "npx @eslint/config-inspector --host 0.0.0.0",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint .",
    "test": "vitest",
    "typegen": "esno scripts/typegen.ts",
    "prepack": "pnpm run build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "prepare": "node .husky/install.mjs"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.50.0",
    "astro-eslint-parser": "^1.0.2",
    "eslint": ">=8.40.0",
    "eslint-plugin-astro": "^1.2.0",
    "prettier-plugin-astro": "^0.14.0"
  },
  "peerDependenciesMeta": {
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.4",
    "@clack/prompts": "^0.7.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/eslint-plugin": "^1.0.1",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-flat-config-utils": "^0.3.0",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.5",
    "eslint-plugin-command": "^0.2.3",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import-x": "^3.1.0",
    "eslint-plugin-jsdoc": "^50.0.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-perfectionist": "^3.1.3",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-toml": "^0.11.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "globals": "^15.9.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^0.5.0",
    "parse-gitignore": "^2.0.0",
    "prettier": "^3.3.3",
    "toml-eslint-parser": "^0.10.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.2",
    "@types/eslint": "^9.6.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.2.0",
    "@unocss/eslint-plugin": "^0.62.1",
    "astro-eslint-parser": "^1.0.2",
    "bumpp": "^9.5.1",
    "eslint": "^9.9.0",
    "eslint-plugin-astro": "^1.2.3",
    "eslint-typegen": "^0.3.0",
    "esno": "^4.7.0",
    "execa": "^9.3.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "prettier-plugin-astro": "^0.14.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
